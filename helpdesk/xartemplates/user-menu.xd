<div id="tabnav">
    
    <xar:foreach in="$menulinks" key="$key" value="$link">
    
    <xar:if condition="$page eq $link.page AND empty($link.type)">
    <div><a href="#$link.url#" class="active">#$link.label#</a></div>
    <xar:else />
    <div><a href="#$link.url#">#$link.label#</a></div>
    </xar:if>
    
    </xar:foreach>
    
</div>