<methodResponse>
  <params>
    <param>
      <value>
        <array>
          <data>
            <xar:if condition="!empty($articlelist)">
              <xar:loop name="$articlelist">
                <value>
                  <struct>
                    <member>
                      <name>dateCreated</name>
                      <value>
                        <dateTime.iso8601>#$loop:item.dateCreated#</dateTime.iso8601>
                      </value>
                    </member>
                    <member>
                      <name>userid</name>
                      <value>
                        <string>#$loop:item.authorid#</string>
                      </value>
                    </member>
                    <member>
                      <name>postid</name>
                      <value>
                        <string>#$loop:item.postid#</string>
                      </value>
                    </member>
                    <member>
                      <name>title</name>
                      <value><string>#$loop:item.title#</string></value>
                    </member>
                  </struct>
                </value>
              </xar:loop>  
            </xar:if>
          </data>  
        </array>
      </value>
    </param>
  </params>
</methodResponse>
