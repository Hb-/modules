<xar:if condition="xarTplAddStyleLink('netquery', $stylesheet)">
</xar:if>

<script type="text/javascript" src="modules/netquery/xartemplates/popup.js"></script>

<div class="xar-mod-head">
  <span class="xar-mod-title"><xar:mlstring>Netquery Administration</xar:mlstring></span>
</div>

<div class="xar-mod-body">
<div class="xar-accent">
  <table class="nqinput">
    <tr>
      <th colspan="3">
          <a href="javascript:popUp('#$hlplink.url#','console','400','600');"><img class="floatbutton" src="modules/netquery/xarimages/btn_help.gif" alt="#$hlplink.label#" /></a>
          <xar:mlstring>Netquery Looking Glass Configuration</xar:mlstring>
      </th>
    </tr>
    <tr>
      <th>
          <xar:mlstring>Router</xar:mlstring>
      </th>
      <th>
          <xar:mlstring>Address</xar:mlstring>
      </th>
      <th>
          <xar:mlstring>Options</xar:mlstring>
      </th>
    </tr>
    <xar:loop name="$items">
    <xar:if condition="(($loop:index + 1) % 2)">
      <tr>
        <td class="xar-norm">
            #$loop:item.router#
        </td>
        <td class="xar-norm">
            #$loop:item.address#
        </td>
        <td class="xar-norm">
           <xar:if condition="empty($loop:item.editurl)">
               #$edittitle#
               <xar:else />
                   <a href="#$loop:item.editurl#">#$loop:item.edittitle#</a>
           </xar:if>
           |
           <xar:if condition="empty($loop:item.deleteurl)">
               #$loop:item.deletetitle#
               <xar:else />
                   <a href="#$loop:item.deleteurl#">#$loop:item.deletetitle#</a>
           </xar:if>
        </td>
      </tr>
    <xar:else />
      <tr>
        <td class="xar-accent">
            #$loop:item.router#
        </td>
        <td class="xar-accent">
            #$loop:item.address#
        </td>
        <td class="xar-accent">
           <xar:if condition="empty($loop:item.editurl)">
               #$loop:item.edittitle#
               <xar:else />
                   <a href="#$loop:item.editurl#">#$loop:item.edittitle#</a>
           </xar:if>
           |
           <xar:if condition="empty($loop:item.deleteurl)">
               #$loop:item.deletetitle#
               <xar:else />
                   <a href="#$loop:item.deleteurl#">#$loop:item.deletetitle#</a>
           </xar:if>
        </td>
      </tr>
    </xar:if>
    </xar:loop>
    <tr>
      <th class="nq-center" colspan="3">
        <a href="#$lgalink.url#">#$lgalink.title#</a> -
        <a href="#$cfglink.url#">#$cfglink.title#</a>
      </th>
    </tr>
  </table>
</div>
</div>
