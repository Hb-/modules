<div class="xar-mod-head"><span class="xar-mod-title">#$menutitle#</span></div>
<br />

<div class="xar-mod-body">
<div style="margin: auto;">

<table style="width:100%; border-collapse:collapse; border-spacing:1px; padding:0;">
    <tr>
    <xar:foreach in="$menulinks" value="$link">
         <th style="text-align: center; padding: 0 .5em 0 .5em;">
            <a href="#$link[1]#">#$link[0]#</a>
         </th>
    </xar:foreach>
    </tr>
</table>


