<xar:if condition="!empty($user_elements) && is_array($user_elements)">
    <xar:if condition="empty($layout) || $layout eq 'default'">
        <xar:foreach in="$user_elements" value="$item">
            <a href="http://twitter.com/#$item.screen_name#" title="#$item.screen_name#">
                <img src="#$item.profile_image_url#" alt="#xarML('profile image')#" title="#$item.screen_name#" />
            </a>
        </xar:foreach>
    <xar:else />

    </xar:if>

</xar:if>
