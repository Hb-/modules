<xar:set name="$weatherdotcomurl">'<a href="http://www.weather.com?prod=xoap&amp;par='.xarModGetVar('weather','partner_id').'"><strong>weather.com&reg;</strong></a>'</xar:set>
<div id="weatherdotcomfooter" style="text-align: center;">
    <div id="weatherdotcomlinks" style="text-align: center; border-bottom: 1px solid black;">
        <xar:ml>
            <xar:mlstring>Featured on #(1)</xar:mlstring>
            <xar:mlvar>#$weatherdotcomurl#</xar:mlvar>
        </xar:ml>
        <br />
        <a href="#$wData.linkOne#">#$wData.titleOne#</a> |
        <a href="#$wData.linkTwo#">#$wData.titleTwo#</a> |
        <a href="#$wData.linkThree#">#$wData.titleThree#</a> |
        <a href="#$wData.linkFour#">#$wData.titleFour#</a>
    </div>
    <div id="weatherdotcomnotice">
        <xar:ml>
            <xar:mlstring>Weather data provided by #(1)</xar:mlstring>
            <xar:mlvar>#$weatherdotcomurl#</xar:mlvar>
        </xar:ml>
    </div>
</div>