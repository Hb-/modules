<xar:base-include-javascript position="head" module="base" filename="xmlhttprequest.js" />
<xar:base-include-javascript position="body" module="xtasks" filename="spinner.js" />
<xar:base-include-javascript position="body" module="base" filename="calendar.js" />
<xar:set name="projects_objectid">xarModGetVar('xproject','projects_objectid')</xar:set>
<xar:data-getitem name="$properties" module="xProject" itemtype="1" itemid="$projects_objectid" />
<xar:style scope="module" file="xproject" />

<xar:set name="mymemberid">xarModGetUserVar('xproject', 'mymemberid')</xar:set>
<xar:set name="dummy">xarVarFetch('module', 'str::', $currentmodname, '', XARVAR_NOT_REQUIRED)</xar:set>
<xar:set name="dummy">xarVarFetch('type', 'str::', $currentmodtype, '', XARVAR_NOT_REQUIRED)</xar:set>
<xar:set name="dummy">xarVarFetch('func', 'str::', $currentmodfunc, '', XARVAR_NOT_REQUIRED)</xar:set>

<xar:if condition="$inline ne 1">
    <table border="0" width="100%">
    
        <tr><th align="center" colspan="2">#$menutitle#</th></tr>
        <tr>
            <td width="65" rowspan="2"><a href="&xar-modurl-xproject-user-main;"><img src="modules/xproject/xarimages/admin.jpg" style="border: 0px;" /></a></td>
            <td>
            <table border="0" width="100%">
            <tr align="center" valign="middle">
                <td width="20%" class="xproject-menu-item">
                    <xar:if condition="$currentmodname eq 'xproject' 
                                        and $currentmodtype eq 'admin'
                                        and $currentmodfunc eq 'overview'">
                        <strong><xar:mlstring>Overview</xar:mlstring></strong>
                    <xar:else />
                        <a href="&xar-modurl-xproject-admin-overview;" class="xproject-menu-link"><xar:mlstring>Overview</xar:mlstring></a>
                    </xar:if>
                </td>
                <td width="20%" class="xproject-menu-item">
                    <xar:if condition="$currentmodname eq 'xproject' 
                                        and $currentmodtype eq 'admin'
                                        and $currentmodfunc eq 'dashboard'">
                        <strong><xar:mlstring>Dashboard</xar:mlstring></strong>
                    <xar:else />
                        <a href="&xar-modurl-xproject-admin-dashboard;" class="xproject-menu-link"><xar:mlstring>Dashboard</xar:mlstring></a>
                    </xar:if>
                </td>
                <td width="20%" class="xproject-menu-item">
                    <xar:if condition="$currentmodname eq 'xproject' 
                                        and $currentmodtype eq 'admin'
                                        and  ( $currentmodfunc eq 'main' or empty($currentmodfunc) )">
                        <strong><xar:mlstring>Search Projects</xar:mlstring></strong>
                    <xar:else />
                        <a href="&xar-modurl-xproject-admin-main;" class="xproject-menu-link"><xar:mlstring>Search Projects</xar:mlstring></a>
                    </xar:if>
                </td>
                <td width="20%" class="xproject-menu-item">
                    <xar:if condition="$currentmodname eq 'xproject' 
                                        and $currentmodtype eq 'admin'
                                        and $currentmodfunc eq 'new'">
                        <strong><xar:mlstring>New Project</xar:mlstring></strong>
                    <xar:else />
                        <a href="&xar-modurl-xproject-admin-new;" class="xproject-menu-link"><xar:mlstring>New Project</xar:mlstring></a>
                    </xar:if>
                </td>
                <td width="20%" class="xproject-menu-item">
                    <xar:if condition="$currentmodname eq 'xproject' 
                                        and ( $currentmodtype eq 'user' or empty($currentmodtype) ) 
                                        and $currentmodfunc eq 'settings'">
                        <strong><xar:mlstring>My Settings</xar:mlstring></strong>
                    <xar:else />
                        <a href="&xar-modurl-xproject-user-settings;" class="xproject-menu-link"><xar:mlstring>My Settings</xar:mlstring></a>
                    </xar:if>
                </td>
            
            </tr></table>
        </td></tr>
        <tr><td>
            <table border="0" width="100%">
            <tr align="center" valign="middle">
                <td width="20%" class="xproject-menu-item">
                    <xar:if condition="$currentmodname eq 'xproject' 
                                        and $currentmodtype eq 'admin' 
                                        and $currentmodfunc eq 'agenda'">
                        <strong><xar:mlstring>Agenda</xar:mlstring></strong>
                    <xar:else />
                        <a href="&xar-modurl-xproject-admin-agenda;" class="xproject-menu-link"><xar:mlstring>Agenda</xar:mlstring></a>
                    </xar:if>
                </td>
                <td width="20%" class="xproject-menu-item">
                    <xar:if condition="$currentmodname eq 'xproject' 
                                        and $currentmodtype eq 'admin' 
                                        and  ( $currentmodfunc eq 'main' or empty($currentmodfunc) )
                                        and $memberid eq $mymemberid
                                        and $status eq $draftstatus">
                        <strong><xar:mlstring>My Drafts</xar:mlstring></strong>
                    <xar:else />
                        <xar:if condition="$mymemberid gt 0"><a href="&xar-modurl-xproject-admin-main;&amp;status=#$draftstatus#&amp;sortby=priority&amp;memberid=#$mymemberid#" class="xproject-menu-link"><xar:mlstring>My Drafts</xar:mlstring></a></xar:if>
                    </xar:if>
                </td>
                <td width="20%" class="xproject-menu-item">
                    <xar:if condition="$currentmodname eq 'xproject' 
                                        and $currentmodtype eq 'admin'
                                        and  ( $currentmodfunc eq 'main' or empty($currentmodfunc) )
                                        and $memberid eq $mymemberid
                                        and $status eq $activestatus">
                        <strong><xar:mlstring>My Active Projects</xar:mlstring></strong>
                    <xar:else />
                        <xar:if condition="$mymemberid gt 0"><a href="&xar-modurl-xproject-admin-main;&amp;status=#$activestatus#&amp;sortby=priority&amp;memberid=#$mymemberid#" class="xproject-menu-link"><xar:mlstring>My Active Projects</xar:mlstring></a></xar:if>
                    </xar:if>
                </td>
                <td width="20%" class="xproject-menu-item">
                    <xar:if condition="$currentmodname eq 'xproject' 
                                        and $currentmodtype eq 'admin' 
                                        and  ( $currentmodfunc eq 'main' or empty($currentmodfunc) )
                                        and $status eq $archivestatus">
                        <strong><xar:mlstring>Archive</xar:mlstring></strong>
                    <xar:else />
                        <a href="&xar-modurl-xproject-admin-main;&amp;status=#$archivestatus#" class="xproject-menu-link"><xar:mlstring>Archive</xar:mlstring></a>
                    </xar:if>
                </td>
                <td width="20%" class="xproject-menu-item">
                    <xar:if condition="$currentmodname eq 'xproject' 
                                        and $currentmodtype eq 'admin'
                                        and $currentmodfunc eq 'modifyconfig'">
                        <strong><xar:mlstring>Configuration</xar:mlstring></strong>
                    <xar:else />
                        <a href="&xar-modurl-xproject-admin-modifyconfig;" class="xproject-menu-link"><xar:mlstring>Configuration</xar:mlstring></a>
                    </xar:if>
                </td>
            
            </tr></table>
        </td></tr>
    
        <xar:if condition="!isset($q)">
            <xar:set name="q">""</xar:set>
        </xar:if>
    
    </table>
</xar:if>
