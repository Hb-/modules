<xar:comment>
    Next/previous links, displayed in the individual event page.
</xar:comment>


<div class="ievents-nextprev">
    <ul>
        <xar:if condition="!empty($prev_event)">
            <li class="ievents-nextprev-prev">
                <a href="#xarServerGetCurrentURL(array('eid'=&gt;$prev_event.eid,'startnum'=&gt;$prev_event.startnum))#" title="#xarVarPrepForDisplay($prev_event.title)#">
                    &lt;&lt; <xar:mlstring>Previous Event</xar:mlstring>
                </a>
            </li>
        <xar:else />
            <li class="ievents-nextprev-prev"><span>&lt;&lt; <xar:mlstring>Previous Event</xar:mlstring></span></li>
        </xar:if>

        <li class="ievents-nextprev-list"><a href="#xarServerGetCurrentURL(array('eid'=&gt;NULL),true,'event-summary-'.$event.eid)#"><xar:mlstring>Back to list</xar:mlstring></a></li>

        <xar:if condition="!empty($next_event)">
            <li class="ievents-nextprev-next">
                <a href="#xarServerGetCurrentURL(array('eid'=&gt;$next_event.eid,'startnum'=&gt;$next_event.startnum))#" title="#xarVarPrepForDisplay($next_event.title)#">
                    <xar:mlstring>Next Event</xar:mlstring> &gt;&gt;
                </a>
            </li>
        <xar:else />
            <li class="ievents-nextprev-next"><span><xar:mlstring>Next Event</xar:mlstring> &gt;&gt;</span></li>
        </xar:if>
    </ul>
    <div style="clear: both;"></div>
</div>
