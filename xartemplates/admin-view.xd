<xar:template type="module" file="admin-menu" />
<xar:data-getitem name="$properties" module="accessmethods" itemtype="1" itemid="$accessmethods_objectid" />
<div class="xar-mod-head"><span class="xar-mod-title"><xar:mlstring>Access Method List</xar:mlstring></span></div>
<div class="xar-mod-body">
<div align="center">

<table border="0" cellpadding="2" cellspacing="1" class="xar-accent" style="width: 100%;">
    <tr>

        <th><xar:mlstring>Access Method Name</xar:mlstring></th>
        <th><xar:mlstring>Type</xar:mlstring></th>
        <th><xar:mlstring>Client</xar:mlstring></th>
        <th><xar:mlstring>SLA</xar:mlstring></th>
        <th><xar:mlstring>Webmaster</xar:mlstring></th>
        <th><xar:mlstring>Options</xar:mlstring></th>
    </tr>

    <xar:loop name="$items">
        <tr><td class="xar-norm">
                <xar:if condition="empty($loop:item['link'])">#$loop:item['site_name']#<xar:else /><a href="#$loop:item['link']#">#$loop:item['site_name']#</a></xar:if>
            </td>
        
            <td class="xar-norm" style="text-align: center;">#$loop:item['accesstype']#</td>
            
            <xar:set name="companyinfo">xarModAPIFunc('addressbook','user','getdetailvalues',array('id'=>$loop->item['clientid']))</xar:set>
            <td class="xar-norm" style="text-align: center;">#$companyinfo['company']#</td>
        
            <td class="xar-norm" style="text-align: center;">#$loop:item['sla']#</td>
            
            <td class="xar-norm" style="text-align: center;">
            <xar:if condition="$loop:item['webmasterid'] gt 0">
                <xar:set name="webmasterinfo">xarModAPIFunc('roles','user','get',array('uid'=>$loop->item['webmasterid']))</xar:set>
                #$webmasterinfo['uname']#
            </xar:if>
            </td>
        
            <td class="xar-norm" style="text-align: center;">[&nbsp;<xar:if condition="empty($loop:item['editurl'])"><xar:mlstring>Edit</xar:mlstring><xar:else /><a href="#$loop:item['editurl']#"><xar:mlstring>Edit</xar:mlstring></a></xar:if>&nbsp;|&nbsp;<xar:if condition="empty($loop:item['deleteurl'])"><xar:mlstring>Delete</xar:mlstring><xar:else /><a href="#$loop:item['deleteurl']#"><xar:mlstring>Delete</xar:mlstring></a></xar:if>&nbsp;]</td>
    
        </tr>
            
    </xar:loop>
    
</table>

<br />

</div>
</div>