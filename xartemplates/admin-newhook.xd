<div class="xar-form-input-wrapper">
    <label for="tb_pingurl" class="xar-form-label help" title="#xarML('The url that you are notifying for trackback.')#"><xar:mlstring>TrackBack Ping URL</xar:mlstring></label>
    <input type="text" name="tb_pingurl" id="tb_pingurl" value="#$pingurl#" size="50" maxlength="200" />
</div>
<div class="xar-form-input-wrapper">
    <label for="tb_excerpt" class="xar-form-label help" title="#xarML('Comment that you wish to attach to the trackback.')#"><xar:mlstring>Excerpt:</xar:mlstring></label>
    <input type="text" name="tb_excerpt" id="tb_excerpt" value="#$excerpt#" size="50" maxlength="200" />
</div>
