<xar:set name="join">""</xar:set>
<xar:foreach in="$options" value="$option">
        <xar:if condition="in_array($option['id'],$value)" >
              #$join# #$option['accesstype']# : <a href="#$option['url']#">#$option['name']#</a>
              <xar:set name="join">"<br>"</xar:set>
        </xar:if>
</xar:foreach>
