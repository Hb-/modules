<xar:data-getitem name="$properties" module="dossier" itemtype="1" itemid="0" />
<xar:data-getitem name="$logproperties" module="dossier" itemtype="4" itemid="0" />

<div class="xar-mod-head"><span class="xar-mod-title"><xar:mlstring>Delete Log Record</xar:mlstring></span></div>
<div class="xar-mod-body">

<form method="post" action="&xar-modurl-dossier-logs-delete;">
    <input type="hidden" name="authid" value="#$authid#" />
    <input type="hidden" name="confirm" value="true" />
    <input type="hidden" name="logid" value="#$logid#" />

    <table border="0" cellpadding="2" cellspacing="0">
        <tr>
            <td align="right" width="30%">
                <xar:mlstring>Contact</xar:mlstring>
            </td>
            <td><strong>#$contactinfo['sortname']#</strong> (ID: #$contactinfo['contactid']#)</td>
        </tr>
        <tr>
            <td align="right" width="20%">
                <xar:data-label property="$logproperties['ownerid']" />
            </td>
            <td>
                <xar:data-output property="$logproperties['ownerid']" value="$item['ownerid']" />
            </td>
        </tr>
        <tr>
            <td align="right" width="20%">
                <xar:data-label property="$logproperties['logtype']" />
            </td>
            <td>
                <xar:data-output property="$logproperties['logtype']" value="$item['logtype']" />
            </td>
        </tr>
        <tr>
            <td align="right" width="20%">
                <xar:data-label property="$logproperties['logdate']" />
            </td>
            <td>
                <xar:data-output property="$logproperties['logdate']" value="$item['logdate']" />
            </td>
        </tr>
        <tr>
            <td align="right" width="20%">
                <xar:data-label property="$logproperties['notes']" />
            </td>
            <td>
                <xar:data-output property="$logproperties['notes']" value="$item['notes']" />
            </td>
        </tr>
        <tr>
            <td colspan="2" align="center"><br />
                    <input type="submit" value="#$confirmbutton#"  />
            </td>
        </tr>
    </table>
</form>
</div>
