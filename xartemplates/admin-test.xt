<?xml version="1.0" encoding="utf-8"?>
<xar:template xmlns:xar="http://xaraya.com/2004/blocklayout">
    <!-- License: GPL http://www.gnu.org/copyleft/gpl.html -->
    <xar:template file="admin-mod-head"/>
    <div class="xar-mod-body">
        <h2>Test Scheduler Job</h2>
        <fieldset>
            <p>
                Run this job?
            </p>
        </fieldset>

        <form method="post" action="#xarModURL('scheduler','admin','delete')#">
            <div class="xar-row">
                <div class="xar-col">
                    &#160;
                </div>
                <div class="xar-col">
                    <input type="hidden" name="authid" id="authid" value="#$authid#"/>
                    <input type="hidden" name="confirm" id="confirm" value="true"/>
                    <input type="hidden" name="itemid" id="itemid" value="#$itemid#"/>
                    <xar:var name="label">Delete Job</xar:var>
                    <xar:button type="submit" label="$label" />
                    <xar:set name="returnurl">
                        #xarModURL('scheduler', 'admin', 'view')#
                    </xar:set>
                    <xar:var name="label">Cancel</xar:var>
                    <xar:button type="cancel" label="$label" onclick="window.location='#$returnurl#';"/>
                </div>
            </div>
        </form>
    </div>
</xar:template>