<xar:template xmlns:xar="http://xaraya.com/2004/blocklayout">
<xar:mlstring>Dear</xar:mlstring> #$adminname#,


<xar:mlstring>A user registered with the following details at: </xar:mlstring> #$sitename#

<xar:set name="lineend">"\r\n"</xar:set>
    <xar:data-getitem name="$properties" object="$object" />
    <xar:foreach in="$properties" key="$field">
        #$properties.$field:label#: #$properties.$field:value# #$lineend#
    </xar:foreach>

#$terms#


<xar:mlstring>When logged in, you can access this user account at </xar:mlstring>#$rolelink#

-------------------------------------------------------------
</xar:template>
