
<xar:style file="mag" scope="module" module="mag" />
<xar:style file="mag-custom" scope="module" module="mag" />

<xar:template file="custom-navbar" module="mag" />

<div class="mag mag-mags">
    <xar:if condition="empty($mags)">
        <xar:template module="mag" file="error-no-mag" />
    <xar:else />
        <xar:if condition="!empty($mid)">
            <xar:loop name="$mags">
                <xar:template file="mag-overview" subdata="array('mag'=&gt;$loop:item)" module="mag" />
            </xar:loop>
        <xar:else />
            <h1><xar:mlstring>All Magazines</xar:mlstring></h1>

            <xar:loop name="$mags">
                <xar:template file="mag-overview" subdata="array('mag'=&gt;$loop:item)" module="mag" />
            </xar:loop>
        </xar:if>
    </xar:if>
</div>
