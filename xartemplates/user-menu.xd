<xar:style scope="module" module="base" file="navtabs"  />
<xar:style scope="module" module="helpdesk" file="style"  />

<dl class="xar-tabs" style="margin-bottom: 0px;">

    <xar:foreach in="$menulinks" key="$key" value="$link">

    <xar:if condition="$page eq $link.page AND empty($link.type)">
    <dd class="active"><a href="#$link.url#">#$link.label#</a></dd>
    <xar:else />
    <dd><a href="#$link.url#">#$link.label#</a></dd>
    </xar:if>

    </xar:foreach>

</dl>