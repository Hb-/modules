<xar:template file="application_top" type="module" module="commerce" />
<xar:template file="adminheader" type="module" module="commerce" />

<div class="xar-mod-body">
    <fieldset>
        <legend>
            <xar:mlstring>
                Suppliers List
            </xar:mlstring>
        </legend>
        <xar:if condition="!empty($message)">
            <p>#xarVarPrepForDisplay($message)#</p>
        <xar:else />
            <xar:set name="returnurl">xarModURL('vendors','admin','suppliers')</xar:set>
            <xar:set name="info">xarModAPIFunc('dynamicdata','user','getobjectinfo',array('name' => 'ice_suppliers'))</xar:set>
            <xar:data-list module="$info['moduleid']" itemtype="$info['itemtype']" fieldlist="id,name,description,url,modified" tplmodule="vendors" extend="true" returnurl="$returnurl" />
        </xar:if>
    </fieldset>
</div>

