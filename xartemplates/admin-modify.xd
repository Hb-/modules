<div class="xar-mod-head"><span class="xar-mod-title"><xar:mlstring>XLink Administration</xar:mlstring></span></div>
<div class="xar-mod-body">
    <xar:template type="module" file="admin-menu" module="xlink" />
    <h2><xar:mlstring>Modify XLink Definition</xar:mlstring></h2>

    <form method="post">
    <fieldset>
        <legend>
            <xar:mlstring>XLink Options</xar:mlstring>
        </legend>
    <xar:data-form object="$object" />
    </fieldset>
    <div class="xar-form-input-wrapper">
        #$hooks#
    </div>
    <div class="xar-form-input-wrapper-after">
        <input type="hidden" name="itemid" id="itemid" value="#$itemid#" />
        <input type="hidden" name="authid" id="authid" value="#$authid#" />
        <input type="submit" name="confirm" value="#$confirm#" />
    </div>

    </form>

</div>

