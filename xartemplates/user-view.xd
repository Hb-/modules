<xar:base-include-javascript module="base" filename="confirmlink.js" position="head" />
<div id="mbreplace">
    <xar:loop name="$bookmarks">
        <xar:if condition="(($loop:index + 1) % 2)">
        <div class="xar-norm" style="padding: 3px;">
            <span style="float: left; text-align: left; width: 89%;">
                <a href="#$loop:item['url']#">
                    #$loop:item['name']#
                </a>
            </span>
            <span style="float: right; text-align: right; width: 9%;">
                <a href="#$loop:item['deleteurl']#" onclick="#$loop:item['javascript']#">
                    X
                </a>
            </span>
        <br />
        </div>
        <xar:else />
        <div class="xar-accent" style="padding: 3px;">
            <span style="float: left; text-align: left; width: 89%;">
                <a href="#$loop:item['url']#">
                    #$loop:item['name']#
                </a>
            </span>
            <span style="float: right; text-align: right; width: 9%;">
                <a href="#$loop:item['deleteurl']#" onclick="#$loop:item['javascript']#">
                    X
                </a>
            </span>
        <br />
        </div>
        </xar:if>
    </xar:loop>
</div>