<div class="xar-mod-head">
    <span class="xar-mod-title">
        <xar:if condition="xarModGetVar('vendors','useModuleAlias')">
            #xarModGetVar('vendors','aliasname')#
        <xar:else />
            <xar:mlstring>View Suppliers</xar:mlstring>
        </xar:if>
    </span>
</div>
<div class="xar-mod-body">
    <h2><xar:mlstring>Supplier List</xar:mlstring></h2>
    <xar:set name="validation">"groups: " . $defaultgroup</xar:set>
    <xar:data-input type="memberlist" object="vendors_vendors" validation="$validation" />
</div>
