                                <xar:else />
                                    <xar:set name="$currMonth">#$i#</xar:set>
                                </xar:if>
                                <xar:set name="$currMonth_long">date("F",mktime(0,0,0,$currMonth,1,2000))</xar:set>
                                <xar:set name="$monthSelected">""</xar:set>                            
