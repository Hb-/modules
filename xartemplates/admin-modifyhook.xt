<div class="xar-form-input-wrapper">
    <label for="#$name_prefix#_#$modid#_1" class="xar-form-label" title="#xarML('Select the categories for this item.')#">
        <xar:var name="$labels['categories']" />
    </label>
    <div class="xar-form-container-after">
        <xar:loop name="$items">
            #$loop:item['select']#
        </xar:loop>
    </div>
</div>
