<xar:template xmlns:xar="http://xaraya.com/2004/blocklayout">
<!-- License: GPL http://www.gnu.org/copyleft/gpl.html -->
<div class="xar-mod-head">
  <span class="xar-mod-title"><xar:mlstring>Translations Administration</xar:mlstring></span>
</div>
<div class="xar-mod-body">
   <xar:template type="module" module="translations" file="admin-nav" />
   <h2><xar:mlstring>Choose a module</xar:mlstring></h2>
   <xar:template file="admin-menu" type="module"/>

<xar:template type="module" file="druidbar" />
<p>
<xar:mlstring>Choose which module you want to translate from the list below.</xar:mlstring>
</p>
<ul>
<xar:loop name="$modlist">
  <li><a href="#xarModURL('translations', 'admin', 'module_overview', array('extid' => $loop:item['regid']))#">
    #$loop:item['name']#&#160;-&#160;#$loop:item['displayname']#
  </a></li>
</xar:loop>
</ul>
</div>
</xar:template>