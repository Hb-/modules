<xar:foreach in="$subitems" key="$objectid" value="$subinfo">
    <div class="xar-form-input-wrapper">
        <label for="si_#$subinfo.objectid#" id="si_#$subinfo.objectid#_label" class="xar-form-label">
            #$subinfo.label#:
        </label>
        <span id="si_#$subinfo.objectid#">
            <xar:mlstring>Please create the item first, after that subitems can be created.</xar:mlstring>
        </span>
    </div>
</xar:foreach>

