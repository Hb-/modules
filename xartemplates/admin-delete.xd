<div class="xar-mod-head">
  <span class="xar-mod-title">
    <xar:mlstring>
      Censor Administration
    </xar:mlstring>
  </span>
</div>
<div class="xar-mod-body" align="center">
     <xar:template module="censor" type="module" file="admin-menu" />
  <h2>
    <xar:mlstring>
      Delete Censored Word
    </xar:mlstring>
  </h2>
  <form name="post" id="post" action="&xar-modurl-censor-admin-delete;" method="post" enctype="application/x-www-form-urlencoded">
    <fieldset>
        <legend>
           <xar:mlstring>Confirm deletion</xar:mlstring>
        </legend>
        <div class="xar-form-input-wrapper">
           <label class="xar-form-label">
               <xar:mlstring>
                    Delete this censored word?
               </xar:mlstring>
           </label>
           <input type="submit" value="#xarML('Delete word')#"/>
        </div>
    </fieldset>
    <div class="xar-form-input-wrapper-after">
        <input type="hidden" name="authid" value="#$authid#" />
        <input type="hidden" name="cid" id="cid" value="#$cid#" />
        <input type="hidden" name="confirm" id="confirm" value="1"/>
        <input type="hidden" name="confirmation" id="confirmation" value="1"/>

        <input type="button" value="#xarML('Cancel')#" onclick="location.href='&xar-modurl-censor-admin-view;'"/>
    </div>
  </form>        
</div>
