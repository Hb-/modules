<methodResponse>
  <params>
    <param>
      <value>
        <array>
          <data>
            <xar:if condition="!empty($categories)">
                <xar:loop name="$categories">
                  <value>
                    <struct>
                      <member>
                        <name>categoryId</name>
                        <value>
                          <int>#$loop:item.categoryId#</int>
                        </value>
                      </member>
                      <member>
                        <name>categoryName</name>
                        <value><string>#$loop:item.categoryName#</string></value>
                      </member>
                    </struct>
                  </value>
                </xar:loop>  
            </xar:if>
          </data>  
        </array>
      </value>
    </param>
  </params>
</methodResponse>