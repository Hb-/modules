

    <xar:template xmlns:xar="dd" xmlns="http://www.w3.org/TR/xhtml1/strict" file="header" type="module"/>
    <div xmlns="http://www.w3.org/TR/xhtml1/strict" xmlns:xar="dd" class="xar-mod-body">
    <div style="padding: 1px;" class="xar-norm-outline">

<form method="post" action="#$action#">

    <input type="hidden" name="authid" id="authid" value="#$authid#"/>

    <table width="100%" cellspacing="0" cellpadding="8" border="0">

        <colgroup>
            <col width="30%"/>
            <col/>
        </colgroup>

    

        <tr>
            <td><label for="supportshorturls"><xar:mlstring>Enable Create Hooks</xar:mlstring></label></td>
            <td xmlns:xar="http://www.w3.org/TR/xhtml1/strict">
                <xar:data-input id="hookenabled_create" type="checkbox" name="hookenabled_create" value="$hookenabled_create"/>
            </td>
        </tr>

        <tr>
            <td><label for="supportshorturls"><xar:mlstring>Create Hooks Function</xar:mlstring></label></td>
            <td xmlns:xar="http://www.w3.org/TR/xhtml1/strict">
                <xar:data-input id="hookfunctions_create" type="checkboxlist" name="hookfunctions_create" value="$hookfunctions_create" validation="$available_hook_functions"/>
            </td>
        </tr>
    
        <tr>
            <td colspan="2" align="center">
                <input type="hidden" name="itemtype" value="#$itemtype#"/>
                <input type="submit" value="#xarML('Modify')#"/>
                <input type="submit" name="cancel" value="#xarML('Cancel')#"/>
            </td>
        </tr>

    </table>
</form>

</div>
</div>


