<xar:template xmlns:xar="http://xaraya.com/2004/blocklayout">
    <!-- License: GPL http://www.gnu.org/copyleft/gpl.html -->
    <div class="xar-mod-head">
        <span class="xar-mod-title">
            <xar:if condition="xarModVars::get('xarayatesting','useModuleAlias')">
                <xar:var scope="module" module="xarayatesting" name="aliasname" />
            <xar:else />
                <xar:mlstring>Xarayatesting Module</xar:mlstring>
            </xar:if>
        </span>
    </div>
    <div class="xar-mod-body">
        <fieldset>
            <xar:if condition="!empty($items)">
                The following modules were checked for PHP validity:
                <ul>
                    <xar:foreach in="$items" value="$item" >
                        <li>
                            #$item['name']#
                        </li>
                    </xar:foreach>
                </ul>
            <xar:else />
                No modules were checked
            </xar:if>
        </fieldset>
        <fieldset>
            <xar:button type="cancel" label="Back"/>
        </fieldset>
    </div>
</xar:template>