                        <input type="hidden" name="custUserData[#$i#][colName]" id="custUserData[#$i#][colName]" value="#$custUserData[$i]['colName']#" />
                    <input name="custUserData[#$i#][label]" id="custUserData[#$i#][type]" value="#$custUserData[$i]['label']#" type="hidden">
