                    <xar:else />
                        <input name="custUserData[#$i#][userData]" id="custUserData[#$i#][userData]" value="" size="9" maxlength="9" />
                    </xar:if>
                        <input type="hidden" name="custUserData[#$i#][type]" id="custUserData[#$i#][type]" value="#$custUserData[$i]['type']#" />
