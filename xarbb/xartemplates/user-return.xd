	<table>
		<tr> 
			<th>
				<xar:mlstring>Information</xar:mlstring>
			</th>
		</tr>
		<tr> 
			<td class="xar-norm"> 
				<p>
					<xar:mlstring>Your message has been entered successfully.</xar:mlstring>
				</p>
				<p>
					<a href="#$topicreturn#">
						<xar:mlstring>Click Here</xar:mlstring>
