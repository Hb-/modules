                    <xar:mlstring>
                           Arrow left gif (name, width, height):
                    </xar:mlstring>
                </label>
            </span>
        </span>
        <span style="float: right; width: 70%; text-align: left;">
            <input type="text" name="leftarrowgifname" value="#$leftarrowgif['name']#" size="10" />&nbsp;&nbsp;
            <input type="text" name="leftarrowgifwidth" value="#$leftarrowgif['width']#" size="4" />&nbsp;&nbsp;
            <input type="text" name="leftarrowgifheight" value="#$leftarrowgif['height']#" size="4" />
        </span>
</p>
     
<p>
    <xar:mlstring>Menu Content</xar:mlstring> 
    <br />
    <xar:mlstring>
        For rollover images set "Title" to:  "rollover:Image1.jpg:Image2.jpg"
    </xar:mlstring>
</p>

<table class="xar-norm-outline">
    <tr>
        <th align="center">
            <xar:mlstring>Title</xar:mlstring>
        </th>
        <th align="center">
            <xar:mlstring>URL</xar:mlstring>
        </th>
        <th align="center">
            <xar:mlstring>Description (optional)</xar:mlstring>
        </th>
        <th align="center">
            <xar:mlstring>Delete</xar:mlstring>
        </th>
        <th align="center">
            <xar:mlstring>Child</xar:mlstring>
        </th>
    </tr>
    <xar:if condition="!empty($menu)">
        <xar:loop name="$menulines">
            <tr>
                <td valign="top">
                    <input type="text" name="linkname[#$loop:index#]" maxlength="255" value="#$loop:item[1]#" class="xar-norm" />
                </td>
                <td valign="top">
                    <input type="text" name="linkurl[#$loop:index#]" maxlength="255" value="#$loop:item[0]#" class="xar-norm" />
                </td>
                <td valign="top">
                    <input type="text" name="linkdesc[#$loop:index#]" maxlength="255" value="#$loop:item[2]#" class="xar-norm" />
                </td>
                <td valign="top">
                    <input type="checkbox" name="linkdelete[#$loop:index#]" value="1" class="xar-norm" />
                </td>
                <td valign="top">
                    <xar:if condition="empty($loop:item[3])">
                        <input type="checkbox" name="linkchild[#$loop:index#]" value="1" class="xar-norm" />
                        <xar:else />
                        <input type="checkbox" name="linkchild[#$loop:index#]" value="1" class="xar-norm" checked="checked" />
                    </xar:if>
                </td>
            </tr>
        </xar:loop>
    </xar:if>
    <tr>
        <td>
            <input type="text" name="new_linkname" maxlength="255" class="xar-norm" />
        </td>
        <td>
            <input type="text" name="new_linkurl" maxlength="255" class="xar-norm" />
        </td>
        <td class="xar-norm">
            <input type="text" name="new_linkdesc" maxlength="255" class="xar-norm" />
        </td>
        <td align="center" class="xar-norm">
            <xar:mlstring>
                New Line 
            </xar:mlstring>
        </td>
        <td class="xar-norm">
            <input type="checkbox" name="new_linkinsert" value="1" class="xar-norm" />
        </td>
    </tr>
</table>
