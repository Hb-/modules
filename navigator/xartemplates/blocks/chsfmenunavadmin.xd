<p style="float:left; width:100%;">
        <span style="float: left; width: 60%; text-align: right;">
            <span class="help" title="#xarML(' Menu orientation - horizontal or vertical')#">
                <label for="firstlinehorizontal">
                    <xar:mlstring>
                           Menu orientation:
                    </xar:mlstring>
                </label>
            </span>
        </span>
        <span style="float: right; width: 40%; text-align: left;">
            <select name="firstlinehorizontal">
                <xar:if condition="$firstlinehorizontal eq 0">
                    <option name="id" value="0" selected>vertical</option>
                <xar:else />
                    <option name="id" value="0">vertical</option>
                </xar:if>
                <xar:if condition="$firstlinehorizontal eq 1">
                    <option name="id" value="1" selected>horizontal</option>
                <xar:else />
                    <option name="id" value="1">horizontal</option>
                </xar:if>
            </select>
        </span>
</p> 
