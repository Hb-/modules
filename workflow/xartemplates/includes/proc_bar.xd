<table cellpadding='0' cellspacing='0'><tr><td>
<div style="height:20px; padding-top:2px; padding-bottom:2px;border:1px solid black; background-color:white; color:black; font-size:10px;">
<table  cellpadding='0' cellspacing='0'>
<tr>
	<td><b>#$proc_info['name']#:#$proc_info['version']#</b>
	<xar:if condition="$proc_info['isValid'] eq 'y'">
		<img border='0' src='modules/workflow/lib/Galaxia/img/icons/green_dot.gif' alt='#xarML('valid')#' title='#xarML('valid')#' />	
	<xar:else/>
		<img border='0' src='modules/workflow/lib/Galaxia/img/icons/red_dot.gif' alt='#xarML('invalid')#' title='#xarML('invalid')#' />
	</xar:if>
	</td>
	<td align='right'>
		<table cellpadding='0' cellspacing='2'>
		<tr>
			<xar:if condition="$proc_info['isActive'] eq 'y'">
			<td><a class="link" href="&xar-modurl-workflow-admin-activities;&amp;pid=#$pid#&amp;deactivate_proc=#$pid#"><img border='0' src='modules/workflow/lib/Galaxia/img/icons/stop.gif' alt='#xarML('stop')#' title='#xarML('stop')#' /></a></td>						
			<xar:else/>
			<xar:if condition="$proc_info['isValid'] eq 'y'">
			<td><a class="link" href="&xar-modurl-workflow-admin-activities;&amp;pid=#$pid#&amp;activate_proc=#$pid#"><img border='0' src='modules/workflow/lib/Galaxia/img/icons/refresh2.gif' alt='#xarML('activate')#' title='#xarML('activate')#' /></a></td>									
			</xar:if>
			</xar:if>
			<td><a class="link" href="&xar-modurl-workflow-admin-activities;&amp;pid=#$pid#"><img border='0' src='modules/workflow/lib/Galaxia/img/icons/Activity.gif' alt='#xarML('activities')#' title='#xarML('activities')#' /></a></td>						
		    <td><a class="link" href="&xar-modurl-workflow-admin-processes;&amp;pid=#$pid#"><img border='0' src='modules/workflow/lib/Galaxia/img/icons/change.gif' alt='#xarML('edit')#' title='#xarML('edit')#' /></a></td>				
		    <td><a class="link" href="&xar-modurl-workflow-admin-shared_source;&amp;pid=#$pid#"><img border='0' src='modules/workflow/lib/Galaxia/img/icons/book.gif' alt='#xarML('code')#' title='#xarML('code')#' /></a></td>		
		    <td><a class="link" href="&xar-modurl-workflow-admin-roles;&amp;pid=#$pid#"><img border='0' src='modules/workflow/lib/Galaxia/img/icons/myinfo.gif' alt='#xarML('roles')#' title='#xarML('roles')#' /></a></td>		
			<td><a href="&xar-modurl-workflow-admin-graph;&amp;pid=#$pid#"><img src='modules/workflow/lib/Galaxia/img/icons/Process.gif' border='0' title='#xarML('graph')#' alt='#xarML('graph')#' /></a></td>		
			<td><a class="link" href="&xar-modurl-workflow-admin-save_process;&amp;pid=#$pid#"><img border='0' src='modules/workflow/lib/Galaxia/img/icons/export.gif' alt='#xarML('export')#' title='#xarML('export')#' /></a></td>

		</tr>
		</table>
	</td>	
</tr>
</table>
</div>
</td><td >
