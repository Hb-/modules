<xar:template file="menu" />

<h2><xar:mlstring>Add a Logger</xar:mlstring></h2>
<p></p>

<table width="100%" border="1" cellspacing="0" cellpadding="4">
    <tr>
           <th align="center">Type</th>
           <th align="center">Description</th>
           <th align="center">Add</th>
    </tr>
     <xar:foreach in="$objects" key="$itemtype" value="$vars">
        <tr>
               <td align="center">#xarML($vars['type'])#</td>
               <td align="center">#xarML($vars['description'])#</td>
               <td align="center"><a href="#xarModURL('logconfig','admin','new',array('itemtype'=>$itemtype))#">Click here</a></td>
        </tr>
    </xar:foreach>
</table>