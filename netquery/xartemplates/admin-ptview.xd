<div class="xar-mod-head">
<span class="xar-mod-title"><xar:mlstring>Netquery Administration</xar:mlstring></span>
</div>
<div class="xar-mod-body">

<h2><xar:mlstring>View-Edit Port Services</xar:mlstring></h2>
<table width="100%" border="0" cellspacing="0" cellpadding="4">
<form method="post" action="#xarModURL('netquery', 'admin', 'ptview')#" method="post" enctype="application/x-www-form-urlencoded" >
    <input type="hidden" name="authid" id="authid" value="#$authid#" />
    <tr><td><label for="portnum">#$portnumlabel#</label>
    <input type="text" name="portnum" id="portnum" value="#$portnum#" size="5" maxlength="10">
    <input type=image height=21 width=35 src="modules/netquery/xarimages/gobutton.gif" align=absMiddle border=0>
    </td></tr>
</form>
</table>
<div class="xar-accent">
<table width="100%" border="0" cellspacing="0" cellpadding="4">
    <tr>
        <th>
            <xar:mlstring>Port</xar:mlstring>
        </th>
        <th>
            <xar:mlstring>Protocol</xar:mlstring>
        </th>
        <th>
            <xar:mlstring>Service</xar:mlstring>
        </th>
        <th>
            <xar:mlstring>Options</xar:mlstring>
        </th>
    </tr>
    <xar:loop name="$items">
    <xar:if condition="(($loop:index + 1) % 2)">
    <tr>
        <td class="xar-norm">
            #$loop:item['port']#
        </td>
        <td class="xar-norm">
            #$loop:item['protocol']#
        </td>
        <td class="xar-norm">
            #$loop:item['service']#
        </td>
        <td align="center" class="xar-norm">
           <xar:if condition="empty($loop:item['editurl'])">
               #$edittitle#
               <xar:else />
                   <a href="#$loop:item['editurl']#">#$loop:item['edittitle']#</a>
           </xar:if>
           |
           <xar:if condition="empty($loop:item['deleteurl'])">
               #$loop:item['deletetitle']#
               <xar:else />
                   <a href="#$loop:item['deleteurl']#">#$loop:item['deletetitle']#</a>
           </xar:if>
        </td>
    </tr>
    <xar:else />
    <tr>
        <td class="xar-accent">
            #$loop:item['port']#
        </td>
        <td class="xar-accent">
            #$loop:item['protocol']#
        </td>
        <td class="xar-accent">
            #$loop:item['service']#
        </td>
        <td align="center" class="xar-accent">
           <xar:if condition="empty($loop:item['editurl'])">
               #$loop:item['edittitle']#
               <xar:else />
                   <a href="#$loop:item['editurl']#">#$loop:item['edittitle']#</a>
           </xar:if>
           |
           <xar:if condition="empty($loop:item['deleteurl'])">
               #$loop:item['deletetitle']#
               <xar:else />
                   <a href="#$loop:item['deleteurl']#">#$loop:item['deletetitle']#</a>
           </xar:if>
        </td>
    </tr>
    </xar:if>
    </xar:loop>
</table>
</div>

<p><a href="#$ptalink['url']#">#$ptalink['title']#</a> -
<a href="#$cfglink['url']#">#$cfglink['title']#</a> -
<a href=javascript:openwindow("#$hlplink['url']#");>#$hlplink['label']#</a></p>
<script language="javascript" type="text/javascript">
    function openwindow(hlpfile)
    {
        window.open (hlpfile,"Help","toolbar=no,location=yes,directories=no,status=no,scrollbars=yes,resizable=no,copyhistory=no,width=600,height=400");
    }
</script>
</div>