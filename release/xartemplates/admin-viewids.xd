<div class="xar-mod-head"><span class="xar-mod-title"><xar:mlstring>Release Module</xar:mlstring></span></div>
<!-- common admin menu for this module - remove link to the current function -->
<div class="xar-mod-body">
<h2><xar:mlstring>View IDs</xar:mlstring></h2>
<table width="100%" border="1" cellspacing="0" cellpadding="4">
    <tr>
        <th>
			<xar:mlstring>ID</xar:mlstring>
		</th>
        <th>
			<xar:mlstring>Name</xar:mlstring>
		</th>
        <th>
			<xar:mlstring>Options</xar:mlstring>
		</th>
	</tr>
    <xar:loop name="$items">
    <tr>
        <td>
			#$rid#
		</td>
        <td>
			#$name#
		</td>
		<td align="center">
           <xar:if condition="empty($editurl)">
               #$edittitle#
               <xar:else />
                   <a href="#$editurl#">#$edittitle#</a>
           </xar:if>
           |
           <xar:if condition="empty($deleteurl)">
               #$deletetitle#
               <xar:else />
                   <a href="#$deleteurl#">#$deletetitle#</a>
           </xar:if>
        </td>
    </tr>
    </xar:loop>
</table>
<br />