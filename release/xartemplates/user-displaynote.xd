<div class="xar-mod-head"><span class="xar-mod-title"><xar:mlstring>Xaraya Web Application Framework</xar:mlstring></span></div>
<!-- common admin menu for this module - remove link to the current function -->
<xar:set name="extlisturl">xarModURL('release','user','view')</xar:set>
<xar:set name="extreleaseurl">xarModURL('release','user','viewnotes')</xar:set>
<div class="xar-mod-body">
<h2><xar:mlstring>Extension Release Details</xar:mlstring></h2>

<table style="width:95%; border:0; cellspacing:0; cellpadding:3; align:center;" class="xar-alt"><tr><td>
  <table style="width:100%; border:0; cellspacing:0; cellpadding:3; align"center;" class="xar-accent"><tr><td>
    <table style="width:100%; border:0; cellspacing:1; cellpadding:1; align"center;" class="xar-norm">
      <tr>
        <td class="xar-normal">
        <div style="width:100%; padding-left:5px;padding-top:5px; padding-;bottom:10px;padding-right:5px;">
            <div style="clear: both; ">
            <span style="float: left; width: 70%; text-align: left;">
                <h3>
                <xar:if condition="$type == 0">
                  <xar:mlstring>Module</xar:mlstring>
                <xar:else />        
                  <xar:mlstring>Theme</xar:mlstring>
                </xar:if>
                :: #$regname#</h3>
            </span>
             <span style="float: right; width: 30%;text-align: right;">
                 <h3>Version: #$version#</h3>
             </span>
            </div>
             <div style="clear: both; width:98%; align:middle;">
             <span style="float: left; width:50%; text-align: left;">
                 <xar:mlstring>Author</xar:mlstring>:
                 <a href="#$contacturl#">#$realname#</a>
             </span>
             <span style="float: right; width: 50%; text-align: right;">
                 <xar:mlstring>Released </xar:mlstring>
                 #xarLocaleFormatDate('%d %B %Y %H:%M:%S %Z', $time)#</p>
             </span>
             </div>
             <div style="clear: both;">
                 <span style="float: left; width: 30%; margin-top:10px;text-align: left; font-size:larger; font-weight:bold;">
                 Name:</span>
                 <span style="float: right; width: 68%; margin-top:10px;text-align: left;">
                    #$displname#
                 </span>
             </div>
             <div style="clear: both;">
                 <span style="float: left; width: 30%; margin-top:10px;text-align: left; font-size:larger; font-weight:bold;">
                 Description:</span>
                 <span style="float: right; width: 68%; margin-top:10px;text-align: left;">
                    #$desc#
                 </span>
             </div>
             <div style="clear: both;">
                 <span style="float: left; width: 30%; margin-top:10px;text-align: left; font-size:larger; font-weight:bold;">
                     Class:
                 </span>
                 <span style="float: right; width: 68%; margin-top:10px;text-align: left;">
                 <xar:if condition="$class == 0">
                   <xar:mlstring>Core</xar:mlstring>
                 <xar:else />        
                   <xar:mlstring>Non-Core</xar:mlstring>
                 </xar:if>
                 </span>
             </div>
             <div style="clear: both;">
                 <span style="float: left; width: 30%; margin-top:10px;text-align: left; font-size:larger; font-weight:bold;">
                     Additional Notes:
                 </span>
                 <span style="float: right; width: 68%; margin-top:10px;text-align: left;">
                     #$notes#
                 </span>
             </div>
             <div style="clear: both; ">
                 <span style="float: left; width: 30%; margin-top:10px;text-align: left; font-size:larger; font-weight:bold;">
                     Changelog:
                 </span>
                 <span style="float: right; width: 68%; margin-top:10px;text-align: left;">
                     #$changelog#
                 </span>
             </div>
             <xar:if condition="!empty($enotes)">
             <div style="clear: both;">
                  <span style="float: left; width: 30%; margin-top:10px;text-align: left; font-size:larger; font-weight:bold;">
                     Editor notes:
                 </span>
                 <span style="float: right; width: 68%; margin-top:10px;text-align: left;">
                     #$enotes#
                 </span>
             </div>
             </xar:if>
             <xar:if condition="!empty($demolink)">
              <div style="clear: both; ">
                 <span style="float: left; width: 30%; margin-top:10px;text-align: left; font-size:larger; font-weight:bold;">
                     Demo Link:
                 </span>
                 <span style="float: right; width: 68%; margin-top:10px;text-align: left;">
                     <a href="#$demolink#">#$demolink#</a>
                 </span>
             </div>
            </xar:if>
              <div style="clear: both; ">
                  <span style="float: left; width: 30%; margin-top:10px;text-align: left; font-size:larger; font-weight:bold;">
                    Download Link:
                 </span>
                 <span style="float: right; width: 68%; margin-top:10px;text-align: left;">
                     <a href="#$dllink#">#$dllink#</a>
                 </span>
             </div>
              <div style="clear: both; ">
                  <span style="float: left; width: 30%; margin-top:10px;text-align: left; font-size:larger; font-weight:bold;">
                    Support Link:
                 </span>
                 <span style="float: right; width: 68%; margin-top:10px;text-align: left;">
                     <a href="#$supportlink#">#$supportlink#</a>
                 </span>
             </div>

             <xar:if condition="$price eq 2">
              <div style="clear: both; ">
                 <span style="float: left; width: 30%; margin-top:10px;text-align: left; font-size:larger; font-weight:bold;">
                    Price:
                 </span>
                 <span style="float: right; width: 68%; margin-top:10px;text-align: left;">
                     #$priceterms#
                 </span>
             </div>
             </xar:if>
              <div style="clear: both; ">
                 <span style="float: left; width: 30%; margin-top:10px; text-align: left; font-size:larger; font-weight:bold;">
                     Certified:
                 </span>
                 <span style="float: right; width: 68%; margin-top:10px;text-align: left;">
                     #$certifiedstatus#
                 </span>
             </div>
              <div style="clear: both; ">
                 <span style="float: left; width: 30%; margin-top:10px;text-align: left; font-size:larger; font-weight:bold;">
                    Status:
                 </span>
                 <span style="float: right; width: 68%; margin-top:10px;text-align: left;">
                     #$stateoption#
                 </span>
             </div>
             </div>
         </td>
      </tr>
    </table>
    </td>
</tr>
</table>
</td></tr></table>
<div style="clear: both; width:98%; ">
<span style="float: left; width: 100%; margin-top:10px; margin-bottom:20px; text-align: center;">
    <a href="#$extlisturl#"><xar:mlstring>Extension List</xar:mlstring></a> ::
    <a href="#$extreleaseurl#"><xar:mlstring>Recent Releases</xar:mlstring></a>
<span>
</div>
<div style="clear: both; width:98%; ">
       #$hooks#
</div>
</div>
</div>
