<xar:template file="user-menu" type="module"/>

<xar:if condition="!empty($status)">
    <strong>#$status#</strong>
</xar:if>

<table width="100%">
	<tr>
    <th>
      <xar:mlstring>Available BitKeeper Repositories</xar:mlstring>
    </th>
  </tr>
  <xar:loop name="$items">
    <tr>
      <td>
        <xar:if condition="empty($loop:item['link'])">
          #$loop:item['reponame']#
        <xar:else />
          <a href="#$loop:item['link']#">#$loop:item['reponame']#</a>
        </xar:if>
      </td>
    </tr>
  </xar:loop>
</table>

