<xar:if condition="xarTplAddStyleLink('base', 'navtabs')">
    <div class="tabnav">
        <div class="navhelp" title="#xarML('Click on a tab to explore further')#">
            <xar:mlstring>
                Options 
            </xar:mlstring>
            : 
        </div>
        <div class="tabnav-hairline"><!-- &nbsp; --></div>
        <ul class="navlist">
            <li>
                <a href="#xarModURL('articles','user','viewmap',array('by' => 'pub'))#">
                    <xar:mlstring>
                        Article Map by Publication Type 
                    </xar:mlstring>
                </a>
            </li>
            <xar:if condition="count($catfilter) eq 2">
                <li>
                    <a href="#xarModURL('articles','user','viewmap',array('by' => 'grid'))#">
                        <xar:mlstring>
                            Article Map by Grid 
                        </xar:mlstring>
                    </a>
                </li>
            </xar:if>
            <xar:if condition="!empty($viewlink)">
                <li>
                    <a href="#$viewlink#">
                        #$viewlabel# 
                    </a>
                </li>
            </xar:if>
            <xar:if condition="!empty($archivelink)">
                <li>
                    <a href="#$archivelink#">
                        #$archivelabel# 
                    </a>
                </li>
            </xar:if>
        </ul>
        <div class="tabnav-hairline"><!-- &nbsp; --></div>
    </div>
</xar:if>