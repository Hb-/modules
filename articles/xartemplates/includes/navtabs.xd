<xar:if condition="xarTplAddStyleLink('articles', 'navtabs')">
    <div class="tabnav">
        <div class="navhelp help" title="#xarML('Click on a tab to display articles from that publication type')#">
            <xar:mlstring>
                Publication Type 
            </xar:mlstring>
            : 
        </div>
        <div class="tabnav-hairline"><!-- &nbsp; --></div>
        <ul class="navlist">
            <xar:foreach in="$pubfilters" value="$pubfilter">
                <xar:if condition="empty($pubfilter['plink'])">
                    <li class="active"><a href="#xarServerGetCurrentURL()#" title="#$pubfilter['ptitle']#"> #$pubfilter['ptitle']# </a></li>
                    <xar:else />
                    <li><a href="#$pubfilter['plink']#" title="#$pubfilter['ptitle']#"> #$pubfilter['ptitle']# </a></li>
                </xar:if>
            </xar:foreach>
        </ul>
        <div class="tabnav-hairline"><!-- &nbsp; --></div>
    </div>
</xar:if>