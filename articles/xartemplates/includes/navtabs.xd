<xar:if condition="xarTplAddStyleLink('articles', 'navtabs')">
	<div id="navhelp" class="help" title="#xarML('Click on a tab to display articles from that publication type')#">
		<xar:mlstring>
			Publication Type 
		</xar:mlstring>
		: 
		<div id="tabnav">
			<ul id="navlist">
				<xar:foreach in="$pubfilters" value="$pubfilter">
					<xar:if condition="empty($pubfilter['plink'])">
						<li id="active">
							<a href="#xarServerGetCurrentURL()#" title="#$pubfilter['ptitle']#" class="active">
								#$pubfilter['ptitle']# 
							</a>
						</li>
						<xar:else />
						<li>
							<a href="#$pubfilter['plink']#" title="#$pubfilter['ptitle']#">
								#$pubfilter['ptitle']# 
							</a>
						</li>
					</xar:if>
				</xar:foreach>
			</ul>
			<div class="tabnav-hairline"><!-- &nbsp; --></div>
		</div>
	</div>
</xar:if>
<br /><br />