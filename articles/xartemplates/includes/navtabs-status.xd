<xar:if condition="xarTplAddStyleLink('articles', 'navtabs') and !empty($statusfilters)">
    <div class="tabnav">
        <div class="navhelp help" title="#xarML('Click on a tab to display articles from that publication type')#">
            <xar:mlstring>
                Status 
            </xar:mlstring>
            : 
        </div>
        <div class="tabnav-hairline"><!-- &nbsp; --></div>
        <ul class="navlist">
            <xar:foreach in="$statusfilters" value="$statusfilter">
                <xar:if condition="empty($statusfilter['slink'])">
                    <li class="active"><a href="#xarServerGetCurrentURL()#" title="#$statusfilter['stitle']#"> #$statusfilter['stitle']# </a></li>
                    <xar:else />
                    <li><a href="#$statusfilter['slink']#" title="#$statusfilter['stitle']#"> #$statusfilter['stitle']# </a></li>
                </xar:if>
            </xar:foreach>
        </ul>
        <div class="tabnav-hairline"><!-- &nbsp; --></div>
    </div>
</xar:if>