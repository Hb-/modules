<xar:if condition="!empty($feature['featuredlabel'])">
        <xar:if condition="!empty($feature['alttitle'])">
            <a href="#$feature['featuredlink']#">#$feature['alttitle']#</a>
        <xar:else />
            <a href="#$feature['featuredlink']#">#xarVarPrepForDisplay($feature['featuredlabel'])#</a>
        </xar:if>
        <br />
        <xar:if condition="$feature['showfeaturedsum'] eq true">
            <xar:if condition="!empty($feature['altsummary'])">
                #$feature['altsummary']#
            <xar:else />
                <xar:if condition="!empty($feature['featureddesc'])">
                    #xarVarPrepHTMLDisplay($feature['featureddesc'])#
                </xar:if>
            </xar:if>
             <br />
        </xar:if>
        <xar:if condition="$feature['showfeaturedbod'] eq true">
                #$feature['featuredbody']#
       </xar:if>
        <br />
</xar:if>


<xar:if condition="!empty($items)">
    <br />
    <div class="xar-block-title"><xar:mlstring>More Featured Headlines</xar:mlstring>:</div>
    <xar:foreach in="$items" value="$item">
        &bull;
        <xar:if condition="empty($item['link'])">
            #xarVarPrepForDisplay($item['label'])#
        <xar:else />
            <a href="#$item['link']#">#xarVarPrepForDisplay($item['label'])#</a>
        </xar:if>
        <xar:if condition="!empty($item['count'])">
            (#$item['count']#)
        </xar:if>
        <br />
        <xar:if condition="!empty($item['desc'])">
            #xarVarPrepHTMLDisplay($item['desc'])#
        <br />
        </xar:if>
    </xar:foreach>
</xar:if>
