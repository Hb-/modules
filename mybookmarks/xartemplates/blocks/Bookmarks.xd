<div xmlns="http://www.w3.org/TR/xhtml1/strict" xmlns:xar="dd">
 <xar:if condition="empty($bookmarks)">
    <center>#$emptymarks#</center>
<xar:else/>
     <xar:loop name="$bookmarks">
