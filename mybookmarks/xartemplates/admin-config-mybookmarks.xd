<xar:template file="admin-header" type="module"/>
<form method="post" action="#$action#">

    <input type="hidden" name="authid" id="authid" value="#$authid#"/>
    <input type="hidden" name="itemtype" id="itemtype" value="#$itemtype#"/>
    <br/>
    <br/>
    <table width="100%">
        <tr>
            <td><label for="integerbox">Item's per page</label></td>
            <td>
                <xar:data-input type="integerbox" name="itemsperpage" value="$itemsperpage"/>
            </td>
        </tr>

    </table>
    <span>
        <br/>
        <input type="submit" value="Modify"/>
        <input type="submit" name="cancel" value="Cancel"/>
    </span>
</form>


